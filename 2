local Tab = Window:MakeTab({Name = "Người Chơi", Icon = "rbxassetid://4483345998", PremiumOnly = false})
getgenv().tele = nil
local Section = Tab:AddSection({Name = "Section"})

local function UpdatePlayerList()
local plrlist = {}
for i, v in pairs(game.Players:GetPlayers()) do
table.insert(plrlist, v.Name)
end
return plrlist
end

local dr = Tab:AddDropdown({Name = "Người Chơi", Default = "1", Options = UpdatePlayerList(), Callback = function(v)
print(v)
getgenv().tele = v
end})

local function RefreshPlayerList()
local newPlrList = UpdatePlayerList()
dr:Refresh(newPlrList, true)
end
spawn(function()
while wait(5) do
RefreshPlayerList()
end
end)
Tab:AddButton({Name = "Dịch Chuyển Vào Người Chơi", Callback = function()
if getgenv().tele and game.Players[getgenv().tele] and game.Players[getgenv().tele].Character then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[getgenv().tele].Character.HumanoidRootPart.CFrame
end 
end})
Tab:AddButton({Name = "Dồn Skill Vào Người Chơi", Callback = function()
if getgenv().tele and game.Players[getgenv().tele] and game.Players[getgenv().tele].Character then
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[plr].Character.HumanoidRootPart.CFrame  
  wait()  
  for i,v in pairs(game.Players:GetDescendants()) do  
  if v:IsA("RemoteEvent") and v.Name == "Fire" then  
  v:FireServer()  
  end  
  end
  end
 end})
Tab:AddToggle({Name = "Góc Nhìn Người Chơi", Default = false,Callback = function(h) 
 getgenv().view = h 
 spawn(function() 
 while view do wait() 
 workspace.CurrentCamera.CameraSubject = game.Players[getgenv().tele].Character 
 if view == false then 
 workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character 
 end
 end
 end)
 end})
  Tab:AddToggle({Name = "Hitbox Người Chơi", Default = false,Callback = function(v) 
  local num = 2500
_G.HeadSize = num
_G.Disabled = v
local function hit()
if _G.Disabled then
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name == getgenv().tele then
pcall(function()
sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius", math.huge)
v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.HumanoidRootPart.Transparency = 5
v.Character.HumanoidRootPart.CanCollide = false
end)
end
end
else num = 0
end
end
game:GetService('RunService').RenderStepped:connect(hit)
end})
   Tab:AddToggle({Name = "Bring Người Chơi", Default = false,Callback = function(a)
  local v2 = nil
getgenv().bri = a
v2 = game:GetService("RunService").RenderStepped:Connect(function()
if bri == false then v2:Disconnect() end
game.Players[getgenv().tele].Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5) wait()
sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius", math.huge)  
  end)  
  end})
