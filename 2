 local Tab = Window:MakeTab({Name = "Skill Độc Nhất", Icon = "rbxassetid://4483345998", PremiumOnly = false})
 Tab:AddButton({Name = "Nút Skill",Callback = function() 
local ContextActionService = game:GetService("ContextActionService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character

-- Hàm xử lý khi người chơi nhấn nút skill
local function handleSkill(actionName, inputState, inputObject)
    -- Kiểm tra xem người chơi có nhấn nút hay không
    if inputState == Enum.UserInputState.Begin then
        -- Lấy tên skill từ actionName
        local skillName = string.sub(actionName, 6)
        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
if v:IsA("Tool") then
if v[skillName]:FindFirstChild("Fire") then
        v[skillName].Fire:FireServer()
else
        v[skillName].RemoteEvent:FireServer()
    end
end
end
end
end

-- Hàm tạo nút skill trên màn hình điện thoại
local function createSkillButton(skillName, position)
    -- Tạo một actionName duy nhất cho mỗi skill
    local actionName = "Skill" .. skillName
    -- Đăng ký hành động với ContextActionService
    ContextActionService:BindAction(actionName, handleSkill, true, Enum.KeyCode[skillName])
    -- Thiết lập hình ảnh cho nút skill
    ContextActionService:SetImage(actionName, "rbxassetid://5754154247") -- Bạn có thể thay đổi id này với hình ảnh khác
    -- Thiết lập vị trí cho nút skill
    ContextActionService:SetPosition(actionName, position)
end

-- Tạo 4 nút skill Z X C V với vị trí tùy ý
createSkillButton("Z", UDim2.new(0.1, 4, 0.8, -200))
createSkillButton("X", UDim2.new(0.1, 4, 0.8, -165))
createSkillButton("C", UDim2.new(0.1, 4, 0.8, -130))
createSkillButton("V", UDim2.new(0.1, 4, 0.8, -90))
createSkillButton("B", UDim2.new(0.1, 4, 0.8, -53))
createSkillButton("N", UDim2.new(0.1, 4, 0.8, -16))
 end})
 Tab:AddToggle({Name = "Kill Aura 300",Default = false,Callback = function(v)
 getgenv().aura= v
local dis = 300
if getgenv().aura == nil then
OrionLib:MakeNotification({
	Name = "Beta Hub!",
	Content = "Địt Mẹ Có Lỗi",
	Image = "rbxassetid://4483345998",
	Time = 5
})
else
while aura do task.wait(.1)
for i, v in pairs(game:GetService("Workspace")["NPC DAMAGE"]:GetDescendants()) do
if v:FindFirstChild("Enemy") and v.Enemy.Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.random(dis) then
pcall(function()
v.Enemy.Health = 0
sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius", math.huge)
end)
end
end
end
end
end})
OrionLib:Init()
